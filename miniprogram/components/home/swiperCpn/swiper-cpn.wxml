<!--components/home/swiperCpn/swiper-cpn.wxml-->
<swiper class="swiper"
        circular
        autoplay
        interval='3000'
        duration='500'
        indicator-dots
        >
<block wx:for="{{list}}" wx:key="index">
<swiper-item class="swiper-item">
<image src="{{item.imagePath}}" mode="widthFix" style="width:100%" ></image> 
</swiper-item>
</block>
</swiper>
<image src=""></image>
